{
  "name": "@tsparticles/preset-template",
  "version": "1.0.0",
  "description": "tsParticles template preset",
  "homepage": "https://particles.js.org/",
  "main": "index.js",
  "jsdelivr": "tsparticles.preset.template.min.js",
  "unpkg": "tsparticles.preset.template.min.js",
  "browser": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "type": "module",
  "scripts": {
    "prettify:src": "prettier --write ./*.js",
    "prettify:readme": "prettier --write ./README.md",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "bundle": "webpack --mode production --config webpack.config.cjs",
    "build": "yarn clear:dist && yarn prettify:src && yarn lint && yarn bundle && yarn prettify:readme",
    "clear:dist": "rimraf -f ./dist",
    "version": "git add package.dist.json",
    "prepack": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsparticles/preset-template.git"
  },
  "keywords": [
    "tsparticles",
    "particles",
    "particle",
    "canvas",
    "jsparticles",
    "xparticles",
    "particles-js",
    "particles.js",
    "particles-ts",
    "particles.ts",
    "javascript",
    "animation",
    "web",
    "html5",
    "web-design",
    "webdesign",
    "css",
    "html",
    "css3",
    "animated",
    "background",
    "tsparticles-preset"
  ],
  "author": "Matteo Bruni",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tsparticles/preset-template/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/eslint-parser": "^7.16.5",
    "babel-loader": "^8.2.3",
    "browserslist": "^4.19.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^5.3.0",
    "webpack": "^5.67.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "tsparticles": "^1.39.0"
  }
}
