{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "tsparticles/wordpress-particles",
  "version": "0.1.0",
  "title": "Wordpress Particles",
  "category": "widgets",
  "icon": "smiley",
  "description": "Official tsParticles WordPress Plugin - Easily create highly customizable particle, confetti and fireworks animations and use them as animated backgrounds for your website.",
  "supports": {
	"html": false
  },
  "attributes": {
	"width": {
	  "type": "string",
	  "default": "100%"
	},
	"height": {
	  "type": "string",
	  "default": "500px"
	},
	"id": {
	  "type": "string",
	  "default": "tsparticles"
	},
	"options": {
	  "type": "string",
	  "default": "{\"background\":{\"color\":\"#000\"},\"fullScreen\":{\"enable\":false},\"particles\":{\"move\":{\"enable\": true}}}"
	}
  },
  "textdomain": "wordpress-particles",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "viewScript": "file:./view.js",
  "style": "file:./style-index.css"
}
