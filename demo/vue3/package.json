  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.3",
    "@types/node": "^17.0.35",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.16.0",
    "eslint-plugin-vue": "^9.0.1",
    "prettier": "^2.6.2",
    "typescript": "~4.7.3",
    "vite": "^2.9.9",
    "vue-tsc": "^0.38.0"
