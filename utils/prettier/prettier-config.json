{
  "$schema": "http://json.schemastore.org/prettierrc",
  "printWidth": 120,
  "endOfLine": "lf",
  "overrides": [
    {
      "files": [
        "*.js",
        "*.ts",
        "*.jsx",
        "*.tsx"
      ],
      "options": {
        "tabWidth": 4,
        "arrowParens": "avoid"
      }
    }
  ]
}

